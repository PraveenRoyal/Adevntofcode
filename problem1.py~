def string_number_finder(args: str) -> list:
    """it takes a string and checks for the
    index of all the numbers that are written
    in strings"""
    numbers_in_words = ['one', 'two', 'three', 'four',
                        'five', 'six', 'seven', 'eight'
                        'nine']
    finder = []
    for a in numbers_in_words:
        m = args.find(a)
        if m == -1:
            continue
        finder.append((m, numbers_in_words.index(a)+1))

    return finder


# alist = string_number_finder('eighttwothree')
# print(alist)


def order_list_parser(a: list, b: list) -> str:
    """here the function merges two lists
    and returns the values from the lists"""
    a.extend(b)
    sorted(a, key=lambda x: x[0])
    (_, ans) = a[:1]
    (_, bns) = a[-1:]
    return str(ans)+str(bns)


first = first_two_digits('eight8twothree')
second = string_number_finder('eight8twothree')


lm = order_list_parser(first, second)
print(lm)

print(first)
first.extend(second)
print(first[:1])
print(first[-1:])
print(first)
print(second)
